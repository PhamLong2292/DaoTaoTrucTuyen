                    SuCoYKhoa.GHICHUBACSI = GhiChuBaSi;
                    SuCoYKhoa.GHICHUKHAC = GhiChuKhac;
                    SuCoYKhoa.NGUOICHUNGKIEN1 = NguoiChungKien1;
